   "source": [
    "# To find factorial\n",
    "a = 5\n",
    "def fact(n):\n",
    "    if a < 0:\n",
    "        return 0\n",
    "    elif a == 1 or a ==0:\n",
    "        return 1\n",
    "    else:\n",
    "        return a * fact(n-1)\n",
    "b =  fact(a)\n",
    "print(b)"
   ]
